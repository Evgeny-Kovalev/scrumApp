{
    "env": {
        "browser":false,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": ["error", "always"],
        "indent": ["error", "tab"],
        "linebreak-style": 0,
        "consistent-return": "off",
        "no-underscore-dangle": "off",
        "no-tabs": "off",
        "max-len": "off",
        "no-console": ["warn", { "allow": ["info", "error"] }]
    }
}
